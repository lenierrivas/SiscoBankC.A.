<tr id="<%= dom_id user %>" class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 hover:bg-gray-50 dark:hover:bg-gray-600">
    <td class="px-6 py-4"><%= user.type_person&.titleize %></td>
    <th scope="row" class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white"><%= user.name %></th>
    <td class="px-6 py-4"><%= user.identification %></td>
    <td class="px-6 py-4"><%= user.tlf %></td>
    <td class="px-6 py-4">
        <%= link_to t('common.see'), user, class: 'user font-medium text-blue-600 dark:text-blue-500 hover:underline' %>
    </td>
</tr>
